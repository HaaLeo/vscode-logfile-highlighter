{
	"name": "LogFileHighlighter",
	"displayName": "Log File Highlighter",
	"description": "Adds color highlighting to log files to make it easier to follow the flow of log events and detect problems.",
	"version": "0.5.6",
	"engines": {
		"vscode": ">=0.9.0-pre.1"
	},
	"publisher": "emilast",
	"contributes": {
		"languages": [{
			"id": "log",
			"aliases": ["Log File", "log"],
			"extensions": [".log"]
		}],
		"grammars": [{
			"language": "log",
			"scopeName": "text.log",
			"path": "./syntaxes/log.tmLanguage"
		}]
	}
}