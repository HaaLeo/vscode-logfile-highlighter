<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>scopeName</key>
		<string>text.log</string>

		<key>fileTypes</key>
		<array>
			<string>log</string>
		</array>

		<key>name</key>
		<string>Log file</string>

		<key>patterns</key>
		<array>
		
			<!-- Log levels -->
			<dict>
				<key>match</key>
				<string>\b(DEBUG)\b</string>
				<key>name</key>

				<string>markup.changed</string>
			</dict>
			<dict>
				<key>match</key>
				<string>\b(HINT|INFO|INFORMATION)\b</string>
				<key>name</key>

				<string>markup.inserted</string>
			</dict>
			<dict>
				<key>match</key>
				<string>\b(WARNING|WARN)\b</string>
				<key>name</key>
				<string>markup.deleted</string>
			</dict>
			<dict>
				<key>match</key>
				<string>\b(ERROR|FAILURE|FAIL)\b</string>
				<key>name</key>
				<string>string.regexp, strong</string>
			</dict>

			<!-- Dates -->
			<dict>
				<key>match</key>
				<string>\d{4}-\d{2}-\d{2}( \d{2}:\d{2}(:\d{2}(,\d{3})?)?)?</string>
				<key>name</key>
				<string>comment</string>
			</dict>

			<!-- Constants -->
			<dict>
				<key>match</key>
				<string>\b([0-9]+|true|false|null)\b</string>
				<key>name</key>
				<string>constant.language</string>
			</dict>

			<!-- String constants -->
			<dict>
				<key>match</key>
				<string>"[^"]*"</string>
				<key>name</key>
				<string>string</string>
			</dict>
	
			<dict>
				<key>match</key>
				<string>'[^']*'</string>
				<key>name</key>
				<string>string</string>
			</dict>

			<!-- Exception type names -->
			<dict>
				<key>match</key>
				<string>\b([a-zA-Z.]*Exception)\b</string>
				<key>name</key>
				<string>string.regexp, emphasis</string>
			</dict>
			
			<!-- Colorize rows of exception call stacks -->
			<dict>
				<key>begin</key>
				<string>^[\t ]*at</string>
				<key>end</key>
				<string>$</string>
				<key>name</key>
				<string>string.key, emphasis</string>
			</dict>

			<!-- Match Urls -->
			<dict>
				<key>match</key>
				<string>\b(http|https|ftp|file)://\S+\b</string>
				<key>name</key>

				<string>constant.language</string>
			</dict>


			<!-- Match character and . sequences (such as namespaces) -->
			<dict>
				<key>match</key>
				<string>\b([\w]+\.)+(\w)+\b</string>
				<key>name</key>

				<string>constant.language</string>
			</dict>
		</array>
		
		<key>uuid</key>
		<string>E81BB6AB-CAC7-4C27-9A79-4137A4693EBD</string>
	</dict>
</plist>
